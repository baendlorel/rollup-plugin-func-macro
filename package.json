{
	"name": "rollup-plugin-func-macro",
	"version": "0.0.3",
	"author": {
		"name": "Kasukabe Tsumugi",
		"email": "futami16237@gmail.com"
	},
	"purpose": "rollup-plugin",
	"description": "Using __func__ like C++ to get current function name(ignores arrow function context)",
	"description_zh": "使用类似C++的__func__来获取当前函数名(忽略箭头函数上下文)",
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		"types": "./dist/index.d.ts",
		"import": "./dist/index.mjs",
		"require": "./dist/index.cjs",
		"default": "./dist/index.mjs"
	},
	"files": [
		"dist"
	],
	"homepage": "https://github.com/baendlorel/rollup-plugin-func-macro#readme",
	"repository": {
		"type": "git",
		"url": "https://github.com/baendlorel/rollup-plugin-func-macro"
	},
	"keywords": [
		"function name",
		"function",
		"macro",
		"cpp",
		"c++",
		"typescript",
		"javascript"
	],
	"scripts": {
		"test": "clear & vitest",
		"lint": "oxlint .",
		"cover": "clear & vitest --coverage",
		"build": "node ./.scripts/rollup.mjs"
	},
	"license": "MIT",
	"devDependencies": {
		"@babel/plugin-proposal-decorators": "^7.28.0",
		"@babel/preset-env": "^7.28.3",
		"@rollup/plugin-alias": "^5.1.1",
		"@rollup/plugin-babel": "^6.0.4",
		"@rollup/plugin-commonjs": "^28.0.6",
		"@rollup/plugin-node-resolve": "^16.0.1",
		"@rollup/plugin-replace": "^6.0.2",
		"@rollup/plugin-terser": "^0.4.4",
		"@rollup/plugin-typescript": "^12.1.4",
		"@types/node": "^24.5.2",
		"@vitest/coverage-v8": "^3.2.4",
		"oxlint": "^1.17.0",
		"prettier": "^3.6.2",
		"rimraf": "^6.0.1",
		"rollup": "^4.52.2",
		"rollup-plugin-dts": "^6.2.3",
		"rollup-plugin-dts-merger": "^1.3.0",
		"tslib": "^2.8.1",
		"typescript": "^5.9.2",
		"vitest": "^3.2.4"
	},
	"dependencies": {
		"@rollup/pluginutils": "^5.3.0",
		"acorn": "^8.15.0",
		"acorn-walk": "^8.3.4"
	}
